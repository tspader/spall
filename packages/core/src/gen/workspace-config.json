{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "workspace": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Workspace name"
        },
        "id": {
          "description": "Cached workspace ID",
          "type": "integer",
          "exclusiveMinimum": 0,
          "maximum": 9007199254740991
        }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "List of included corpora"
    }
  },
  "required": [
    "workspace",
    "include"
  ],
  "additionalProperties": false
}